<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.3.23.11" id="SpringBoardPageDef"
                Package="com.company.myorders.SpringBoard">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <iterator Binds="root" RangeSize="25" DataControl="OrdersService" id="OrdersServiceIterator"/>
    <accessorIterator MasterBinding="OrdersServiceIterator" Binds="orders" RangeSize="25" DataControl="OrdersService"
                      BeanClass="com.company.myorders.mobile.model.Orders" id="ordersIterator"/>
    <iterator Binds="root" RangeSize="25" DataControl="ApplicationFeatures" id="ApplicationFeaturesIterator"/>
    <accessorIterator MasterBinding="ApplicationFeaturesIterator" Binds="features" RangeSize="25"
                      DataControl="ApplicationFeatures" BeanClass="oracle.adfmf.framework.FeatureInformation"
                      id="featuresIterator"/>
  </executables>
  <bindings>
    <attributeValues IterBinding="ordersIterator" id="partyName">
      <AttrNames>
        <Item Value="partyName"/>
      </AttrNames>
    </attributeValues>
    <tree IterBinding="featuresIterator" id="features">
      <nodeDefinition DefName="oracle.adfmf.framework.FeatureInformation" Name="features0">
        <AttrNames>
          <Item Value="id"/>
          <Item Value="image"/>
          <Item Value="name"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <methodAction id="gotoFeature" RequiresUpdateModel="true" Action="invokeMethod" MethodName="gotoFeature"
                  IsViewObjectMethod="false" DataControl="ApplicationFeatures"
                  InstanceName="data.ApplicationFeatures.dataProvider">
      <NamedData NDName="featureId" NDValue="#{pageFlowScope.FeatureId}" NDType="java.lang.String"/>
    </methodAction>
  </bindings>
</pageDefinition>
