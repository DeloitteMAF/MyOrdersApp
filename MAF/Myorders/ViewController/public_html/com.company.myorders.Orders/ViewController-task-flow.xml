<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="ViewController-task-flow">
    <default-activity>router1</default-activity>
    <managed-bean id="__1">
      <managed-bean-name>OrdersManagedBean</managed-bean-name>
      <managed-bean-class>com.company.myorders.application.OrdersManagedBean</managed-bean-class>
      <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__17">
      <managed-bean-name>PushRegisterBean</managed-bean-name>
      <managed-bean-class>com.company.myorders.mobile.PushRegisterBean</managed-bean-class>
      <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <view id="DashBoard">
      <page>/DashBoardPG.amx</page>
    </view>
    <view id="SpringBoard">
      <page>/com.company.myorders.SpringBoard/SpringBoard.amx</page>
    </view>
    <view id="Settings">
      <page>/com.company.myorders.Settings/Settings.amx</page>
    </view>
    <view id="Details">
      <page>/Details.amx</page>
    </view>
    <view id="lineDetails">
      <page>/OrderLines.amx</page>
    </view>
    <method-call id="getOrders">
      <method>#{applicationScope.OrdersManagedBean.initiateDashboard}</method>
      <outcome>
        <fixed-outcome>goToDashboard</fixed-outcome>
      </outcome>
    </method-call>
    <view id="Search">
      <page>/com.company.myorders.Search/Search.amx</page>
    </view>
    <router id="router1">
      <case id="__21">
        <expression>#{preferenceScope.application.userPref.isDeviceRegistered == true}</expression>
        <outcome>registered</outcome>
      </case>
      <case id="__22">
        <expression>#{preferenceScope.application.userPref.isDeviceRegistered == false}</expression>
        <outcome>registerNow</outcome>
      </case>
      <default-outcome>registered</default-outcome>
    </router>
    <method-call id="PushRegisterBean">
      <method>#{applicationScope.PushRegisterBean.registerDevice}</method>
      <outcome>
        <fixed-outcome>goToOrders</fixed-outcome>
      </outcome>
    </method-call>
    <router id="router2">
      <case id="__23">
        <expression>#{applicationScope.OrdersManagedBean.comingFromNotification==true}</expression>
        <outcome>goToOrder</outcome>
      </case>
      <case id="__24">
        <expression>#{applicationScope.OrdersManagedBean.comingFromNotification!=true}</expression>
        <outcome>getAllOrder</outcome>
      </case>
      <default-outcome>getAllOrder</default-outcome>
    </router>
    <view id="NotificationHistory">
      <page>/NotificationHistory.amx</page>
    </view>
    <view id="GraphDashBoard">
      <page>/GraphDashBoard.amx</page>
    </view>
    <control-flow-rule id="__2">
      <from-activity-id>DashBoard</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>openDetail</from-outcome>
        <to-activity-id>Details</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__12">
        <from-outcome>search</from-outcome>
        <to-activity-id>Search</to-activity-id>
        <transition>slideUp</transition>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>Details</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>backToDashBoard</from-outcome>
        <to-activity-id>DashBoard</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__6">
        <from-outcome>openLineDetails</from-outcome>
        <to-activity-id>lineDetails</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__7">
      <from-activity-id>lineDetails</from-activity-id>
      <control-flow-case id="__8">
        <from-outcome>backToDetails</from-outcome>
        <to-activity-id>Details</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__9">
        <from-outcome>Home</from-outcome>
        <to-activity-id>getOrders</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__10">
      <from-activity-id>getOrders</from-activity-id>
      <control-flow-case id="__11">
        <from-outcome>goToDashboard</from-outcome>
        <to-activity-id>GraphDashBoard</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__13">
      <from-activity-id>Search</from-activity-id>
      <control-flow-case id="__14">
        <from-outcome>back</from-outcome>
        <to-activity-id>DashBoard</to-activity-id>
        <transition>slideDown</transition>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__15">
      <from-activity-id>router1</from-activity-id>
      <control-flow-case id="__16">
        <from-outcome>registered</from-outcome>
        <to-activity-id>router2</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__18">
        <from-outcome>registerNow</from-outcome>
        <to-activity-id>PushRegisterBean</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__19">
      <from-activity-id>PushRegisterBean</from-activity-id>
      <control-flow-case id="__20">
        <from-outcome>goToOrders</from-outcome>
        <to-activity-id>router2</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__25">
      <from-activity-id>router2</from-activity-id>
      <control-flow-case id="__26">
        <from-outcome>getAllOrder</from-outcome>
        <to-activity-id>getOrders</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__27">
        <from-outcome>goToOrder</from-outcome>
        <to-activity-id>Details</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__28">
      <from-activity-id>GraphDashBoard</from-activity-id>
      <control-flow-case id="__29">
        <from-outcome>goToFilteredDashboard</from-outcome>
        <to-activity-id>DashBoard</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
