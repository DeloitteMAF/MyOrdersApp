<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="TransactionTF">
    <default-activity>getTrans</default-activity>
    <managed-bean id="__1">
      <managed-bean-name>OrdersManagedBean</managed-bean-name>
      <managed-bean-class>com.company.myorders.application.OrdersManagedBean</managed-bean-class>
      <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <view id="Dashboard">
      <page>/DashboardTransPG.amx</page>
    </view>
    <method-call id="getTrans">
      <method>#{applicationScope.OrdersManagedBean.initiateDashboardTrans}</method>
      <outcome>
        <fixed-outcome>gotoDashboard</fixed-outcome>
      </outcome>
    </method-call>
    <view id="TransLines">
      <page>/TransLines.amx</page>
    </view>
    <!--<view id="SearchTrans">
      <page>/SearchTrans.amx</page>
    </view>-->
    <view id="GraphTransDB">
      <page>/GraphTransDB.amx</page>
    </view>
    <control-flow-rule id="__2">
      <from-activity-id>getTrans</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>gotoDashboard</from-outcome>
        <to-activity-id>GraphTransDB</to-activity-id>
        <transition>flipUp</transition>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>Dashboard</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>openDetail</from-outcome>
        <to-activity-id>TransLines</to-activity-id>
        <transition>flipUp</transition>
      </control-flow-case>
      <!--<control-flow-case id="__10">
        <from-outcome>search</from-outcome>
        <to-activity-id>SearchTrans</to-activity-id>
      </control-flow-case>-->
      <control-flow-case id="__10">
        <from-outcome>backToGraph</from-outcome>
        <to-activity-id>GraphTransDB</to-activity-id>
        <transition>flipDown</transition>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>TransLines</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>backToDashboard</from-outcome>
        <to-activity-id>Dashboard</to-activity-id>
        <transition>flipDown</transition>
      </control-flow-case>
    </control-flow-rule>
    <!--<control-flow-rule id="__8">
      <from-activity-id>SearchTrans</from-activity-id>
      <control-flow-case id="__9">
        <from-outcome>back</from-outcome>
        <to-activity-id>Dashboard</to-activity-id>
      </control-flow-case>
    </control-flow-rule>-->
    <control-flow-rule id="__8">
      <from-activity-id>GraphTransDB</from-activity-id>
      <control-flow-case id="__9">
        <from-outcome>goToFilteredDashboard</from-outcome>
        <to-activity-id>Dashboard</to-activity-id>
        <transition>flipUp</transition>
      </control-flow-case>
    </control-flow-rule>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
