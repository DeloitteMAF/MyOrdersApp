<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="Logout">
    <default-activity>LogoutCall</default-activity>
    <managed-bean id="__1">
      <managed-bean-name>OrdersManagedBean</managed-bean-name>
      <managed-bean-class>com.company.myorders.mobile.OrdersManagedBean</managed-bean-class>
      <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <method-call id="LogoutCall">
      <method>#{applicationScope.OrdersManagedBean.Logout}</method>
      <outcome>
        <to-string/>
      </outcome>
    </method-call>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
