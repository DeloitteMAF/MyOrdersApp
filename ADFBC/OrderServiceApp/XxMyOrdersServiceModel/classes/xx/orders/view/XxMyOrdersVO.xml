<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="XxMyOrdersVO"
  Version="12.1.3.13.26"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  RowClass="xx.orders.view.XxMyOrdersVORowImpl"
  SdoGen="true"
  SdoName="xx.orders.common.XxMyOrdersVOSDO"
  SdoNameSpace="/xx/orders/common/">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_codeGenFlag2" Value="Access"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="xx.orders.view.XxMyOrdersVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bindCustAccountId"
    Kind="where"
    Type="java.lang.Long"/>
  <Variable
    Name="bindNoOfDays"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="bindNoOfRows"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="bindLocation"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[select * from(       
SELECT ooh.order_number order_number, hp.party_name customer,       
          hca.account_number customer_number,       
          ooh.flow_status_code status, ooh.ordered_date, ooh.cust_po_number,       
          xxtest_get_total_value (ooh.header_id) as total_ordered_value,       
          ool.fulfillment_date       
     FROM oe_order_headers_all ooh,       
          oe_order_lines_all ool,       
          hz_parties hp,       
          hz_party_sites hps,       
          hz_locations hl,       
          hz_cust_accounts_all hca,       
          hz_cust_acct_sites_all hcs,       
          hz_cust_site_uses_all hcu       
    WHERE ooh.header_id = ool.header_id       
      AND ooh.org_id = hcu.org_id       
      AND ooh.sold_to_org_id = hca.cust_account_id       
      AND ool.ship_to_org_id = hcu.site_use_id       
      AND hcu.site_use_code = 'SHIP_TO'       
      AND hca.party_id = hp.party_id       
      AND hca.cust_account_id = hcs.cust_account_id       
      AND hcs.cust_acct_site_id = hcu.cust_acct_site_id       
      AND hcs.party_site_id = hps.party_site_id       
      AND hp.party_id = hps.party_id       
      AND hps.location_id = hl.location_id       
      AND hcu.status = 'A'       
      AND hcs.status = 'A'       
      AND hca.status = 'A'       
      AND hp.status = 'A'       
      AND hps.status = 'A'       
      AND hl.country = NVL(:bindLocation,'US')       
      AND   TRUNC (SYSDATE)       
                - NVL (TRUNC (ool.fulfillment_date), TRUNC (SYSDATE)) <= :bindNoOfDays       
      AND hca.cust_account_id = :bindCustAccountId       
 GROUP BY ooh.order_number, hp.party_name, hca.account_number, ooh.flow_status_code,        
          ooh.ordered_date, ooh.cust_po_number, ooh.header_id, fulfillment_date           
 ORDER BY total_ordered_value desc)       
 where rownum<=:bindNoOfRows]]></SQLQuery>
