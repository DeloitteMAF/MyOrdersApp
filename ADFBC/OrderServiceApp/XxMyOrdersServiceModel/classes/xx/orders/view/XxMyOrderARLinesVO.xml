  <SQLQuery><![CDATA[SELECT nvl(applied_customer_trx_id,rct.customer_trx_id) customer_trx_id,rctl.line_number, rctl.quantity_invoiced, rctl.description,          
       rctl.unit_selling_price, flv.meaning status, app.amount_applied amount,          
       app.application_type, aps.due_date,          
       aps.amount_due_original invoice_amount,          
       aps.amount_due_remaining invoice_due_amount   ,      
      CASE when aps.due_date < sysdate then 'Y'                  
            else 'N'      
       END as alert_flag      
  FROM ra_customer_trx_all rct,          
       ra_customer_trx_lines_all rctl,          
       ar_receivable_applications_all app,          
       ar_payment_schedules_all aps,          
       fnd_lookup_values flv          
 WHERE 1 = 1          
   AND rct.customer_trx_id = rctl.customer_trx_id          
   AND app.applied_customer_trx_id = rct.customer_trx_id          
   AND app.applied_payment_schedule_id = aps.payment_schedule_id          
   AND flv.lookup_type = 'PAYMENT_TYPE'          
   AND flv.lookup_code = app.status          
   AND flv.LANGUAGE = USERENV ('lang')          
   AND flv.enabled_flag = 'Y'       
   AND rctl.line_type = 'LINE']]></SQLQuery>
