  <Variable
    Name="bindNoOfDays"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="bindUserName"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT ooh.order_number order_number, hp.party_name customer,                    
          hca.account_number customer_number,                    
          xxmyorders_get_order_status (ooh.header_id) status, ooh.ordered_date, ooh.cust_po_number,                    
          xxmyorders_get_total_value (ooh.header_id) as total_ordered_value,                    
          ooh.header_id,  
          ooh.transactional_curr_code currency_code  
     FROM oe_order_headers_all ooh,                    
          oe_order_lines_all ool,                    
          hz_parties hp,                    
          hz_party_sites hps,                    
          hz_locations hl,                    
          hz_cust_accounts_all hca,                    
          hz_cust_acct_sites_all hcs,                    
          hz_cust_site_uses_all hcu,
          xxmyorder_user_details xud                    
    WHERE ooh.header_id = ool.header_id                    
      AND ooh.org_id = hcu.org_id                    
      AND ooh.sold_to_org_id = hca.cust_account_id                    
      AND ool.ship_to_org_id = hcu.site_use_id                    
      AND hcu.site_use_code = 'SHIP_TO'                    
      AND hca.party_id = hp.party_id                    
      AND hca.cust_account_id = hcs.cust_account_id                    
      AND hcs.cust_acct_site_id = hcu.cust_acct_site_id                    
      AND hcs.party_site_id = hps.party_site_id                    
      AND hp.party_id = hps.party_id                    
      AND hps.location_id = hl.location_id                    
      AND hcu.status = 'A'                    
      AND hcs.status = 'A'                    
      AND hca.status = 'A'                    
      AND hp.status = 'A'                    
      AND hps.status = 'A'                    
      AND hl.country = NVL((xud.country),'US')
      AND hca.cust_account_id = xud.customer_id
      AND xud.user_name = :bindUserName                  
      AND   decode(:bindNoOfDays,-1,-1,(TRUNC (SYSDATE)                    
                - NVL (TRUNC (ooh.ordered_date), TRUNC (SYSDATE)))) <= :bindNoOfDays  
 GROUP BY ooh.order_number, hp.party_name, hca.account_number, ooh.flow_status_code,                     
          ooh.ordered_date, ooh.cust_po_number, ooh.header_id, ooh.transactional_curr_code   
 ORDER BY total_ordered_value desc]]></SQLQuery>
